{
  "name": "@ltipton/parkin",
  "version": "0.0.1",
  "description": "Parse gherkin features and map them to step definitions",
  "main": "build/cjs",
  "module": "build/esm",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/lancetipton/parkin.git"
  },
  "keywords": [
    "gherkin",
    "feature",
    "cucumber",
    "browser",
    "step definition",
    "step definitions"
  ],
  "author": "Lance Tipton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lancetipton/parkin/issues"
  },
  "homepage": "https://github.com/lancetipton/parkin",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build"
  ],
  "engines": {
    "node": ">= 12.0.0"
  },
  "scripts": {
    "prebuild": "yarn test",
    "build": "yarn build:clean && yarn build:rollup",
    "build:clean": "rimraf ./build/esm && rimraf ./build/umd && rimraf ./build/cjs",
    "build:rollup": "NODE_ENV=production rollup -c ./configs/rollup.config.js",
    "clean": "yarn clean:lock && yarn cache clean && rimraf node_modules && npm cache clean --force",
    "clean:cache": "yarn cache clean",
    "clean:full": "yarn clean && yarn install",
    "clean:lock": "rimraf ./yarn.lock && rimraf ./package.lock",
    "dev": "rollup -c ./configs/rollup.config.js -w",
    "format:eslint": "eslint --config ./configs/eslintrc.config.js . --fix --quiet",
    "format:prettier": "prettier --config ./configs/prettier.config.js --ignore-path .eslintignore --write '**/*.{js,jsx}'",
    "format": "yarn format:prettier && yarn format:eslint",
    "test": "NODE_ENV=test jest --detectOpenHandles --no-cache --verbose --config=./configs/jest.config.js"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@keg-hub/jsutils": "8.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.12.1",
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.12.1",
    "@babel/plugin-proposal-optional-chaining": "7.12.7",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-react": "7.12.1",
    "@babel/runtime": "7.12.1",
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-node-resolve": "7.1.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "24.9.0",
    "core-js": "3.6.4",
    "eslint": "7.4.0",
    "eslint-plugin-jest": "23.18.0",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "rollup": "2.23.1",
    "rollup-jest": "0.0.2",
    "rollup-plugin-cleanup": "3.1.1",
    "rollup-plugin-sourcemaps": "0.6.2",
    "rollup-plugin-terser": "7.0.2"
  }
}
